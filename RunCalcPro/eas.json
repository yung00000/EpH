{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "distribution": "store",                
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      },
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "@EXPO_PUBLIC_API_BASE_URL",
        "EXPO_PUBLIC_API_KEY": "@EXPO_PUBLIC_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}